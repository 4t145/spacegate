{
  "gateway_name": "gateway",
  "rules": [
    {
      "hostnames": [
        "localhost",
        "127.0.0.1",
        "0.0.0.0",
        "[::]"
      ],
      "matches": [
        {
          "path": {
            "kind": "Prefix",
            "value": "/"
          }
        }
      ],
      "backends": [
        {
          "name_or_host": "127.0.0.1",
          "port": 8080,
          "protocol": "http",
          "weight": 1
        }
      ]
    }
  ]
}